{
  "name": "memux",
  "version": "0.3.1",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "pretest": "npm run clean && tsc --inlineSourceMap --outDir lib",
    "test": "nyc ava --verbose",
    "posttest": "npm run clean && npm run build",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublish": "npm run build"
  },
  "authors": [
    "Joost Verdoorn <jpverdoorn@gmail.com>",
    "Steffan Sluis <steffansluis@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@reactivex/rxjs": "^5.4.2",
    "@types/node": "^8.0.10",
    "@types/node-uuid": "^0.0.28",
    "no-kafka": "^3.2.0",
    "node-uuid": "^1.4.8",
    "p-queue": "^1.1.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "codecov": "^1.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": "git@github.com:knowledge-express/memux.git"
}
